<div class="admin-form">
  <h4><%= @page_header %></h4>
  <%= form_for(@service, url: admin_services_new_path, method: :post) do |f| %>

    <div class="form-group">
      <%= f.text_field :name, class: "form-control", placeholder: "Service Name" %>
    </div>

    <div class="form-group">
      <%= f.text_field :display_name, class: "form-control", placeholder: "Service Display Name" %>
    </div>

    <div class="form-group">
      <%= f.text_field :rate, class: "form-control", placeholder: "Rate" %>
    </div>

    <div class="form-group">
      <%= f.text_field :category, class: "form-control", placeholder: "Category" %>
    </div>

    <div class="form-group">
      <%= f.text_field :qty, class: "form-control", placeholder: "Default Qty" %>
    </div>

    <%= f.submit 'Submit', class: 'btn btn-primary btn-block' %>

  <% end %>
</div>

